{
  "expo": {
    "name": "ITHS Lab 2",
    "slug": "iths-react-native-lab2",
    "scheme": "ithslab2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "com.iths.lab2"
    },
    "web": {
      "bundler": "webpack",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a1b2c3d4-xxxx-yyyy-zzzz-1234567890ab"
      }
    }
  }
}
